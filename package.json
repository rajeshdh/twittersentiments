{
  "name": "himachalibot",
  "description": "A twitter bot that keeps a watch on hashtags and retweets them",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajeshdh/twittersentiments.git"
  },
  "keywords": [
    "twitter",
    "tweet-bot",
    "sentiment",
    "node.js"
  ],
  "bugs": {
    "url": "https://github.com/rajeshdh/twittersentiments/issues"
  },
  "scripts": {
    "prestart": "babel-node server/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src test:watch",
    "open:src": "babel-node server/index.js",
    "lint": "esw webpack.config.* src server --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress server/testSetup.js \"src/**/*.test.js\" \"server/**/*.test.js\" ",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Rajesh Dhiman @paharihacker",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.2.4",
    "dotenv": "^5.0.1",
    "joi": "^13.3.0",
    "sentiment": "^5.0.0",
    "twit": "^2.2.9",
    "twitter": "^1.7.1",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "13.0.8",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
